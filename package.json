    "parser",
    "require",
    "resolver",
    "scan",
    "scanner",
    "source",
    "static"
  ],
  "bin": {
    "find-requires": "./bin/find-requires"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/medikoo/find-requires.git"
  },
  "dependencies": {
    "es5-ext": "~0.10.4",
    "esniff": "~0.1.1"
  },
  "devDependencies": {
    "tad": "0.2.x",
    "xlint": "~0.2.1",
